{
  "name": "astrowind",
  "description": "A template to make a website using Astro + Tailwind CSS + GitHub Pages",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "bootstrap": "npm install && npm run astro sync && npm run astro check",
    "format": "prettier -w .",
    "format-ci": "prettier --check .",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.astro",
    "all": "npm run format && npm run lint:eslint && npm run build"
  },
  "devDependencies": {
    "@astrojs/image": "=0.17.0",
    "@astrojs/mdx": "^0.19.7",
    "@astrojs/partytown": "^1.2.3",
    "@astrojs/rss": "^2.4.3",
    "@astrojs/sitemap": "^1.3.3",
    "@astrojs/tailwind": "^4.0.0",
    "@astrolib/analytics": "^0.3.0",
    "@astrolib/seo": "^0.4.0",
    "@fontsource/inter": "^4.5.15",
    "@iconify-json/flat-color-icons": "^1.1.6",
    "@iconify-json/icon-park": "^1.1.11",
    "@iconify-json/mdi": "^1.1.52",
    "@iconify-json/ri": "^1.1.10",
    "@iconify-json/tabler": "^1.1.81",
    "@tailwindcss/typography": "^0.5.9",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.59.7",
    "astro": "^2.7.2",
    "astro-compress": "^1.1.49",
    "astro-icon": "^1.0.0-next.2",
    "eslint": "^8.44.0",
    "eslint-plugin-astro": "^0.27.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "limax": "2.1.0",
    "mdast-util-to-string": "^3.1.1",
    "prettier": "^3.0.0",
    "prettier-plugin-astro": "^0.10.0",
    "reading-time": "^1.5.0",
    "sharp": "^0.32.1",
    "svgo": "2.8.0",
    "tailwindcss": "^3.1.2",
    "typescript": "^5.0.3"
  },
  "engines": {
    "node": ">=19.7.0"
  },
  "dependencies": {
    "@astrojs/react": "^2.2.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.13.7",
    "@types/react": "^18.2.11",
    "@types/react-dom": "^18.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-photo-album": "^2.2.2"
  }
}
